  ],
  "engines": {
    "node": ">=18.x.x <=21.x.x",
    "npm": ">=10.0.0"
  },
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/am2222/mapbox-pmtiles",
  "devDependencies": {
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
